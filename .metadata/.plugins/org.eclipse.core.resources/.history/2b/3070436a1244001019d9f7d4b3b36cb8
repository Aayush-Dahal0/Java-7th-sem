package differentSwingComponent;

import javax.swing.JButton;
import javax.swing.JDesktopPane;
import javax.swing.JFrame;
import javax.swing.JInternalFrame;

public class DesktopPaneDemo extends JFrame {
JDesktopPane dp;
JInternalFrame if1,if2,if3,if4;
JButton btn1,btn2,btn3,btn4;

	public DesktopPaneDemo() {
		dp=new JDesktopPane();
		btn1=new JButton();
		btn2=new JButton();
		btn3=new JButton();
		btn4=new JButton();
		if1=new JInternalFrame("First Internal Frame",true,true,true,true);
if1.setLocation(20,20);
if1.add(btn1);
if1.setVisible(true);
add(dp);
setSize(600,600);
setVisible(true);
	}

	public static void main(String[] args) {

	}

}
